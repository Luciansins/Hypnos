{
   "app":{
      "background":{
         "scripts":[
            "wsc-chrome.min.js",
            "background.js"
         ]
      }
   },
   "manifest_version":2,
   "name":"Hypnos",
   "version":"0.1",
   "description":"Notification for Finesse",
   "icons":{
      "16":"images/get_started16.png",
      "32":"images/get_started32.png",
      "48":"images/get_started48.png",
      "128":"images/get_started128.png"
   },
   "permissions":[
      "alarms",
      "unlimitedStorage",
      "storage",
      "notifications",
      "browser",
      "power",
      "system.network",
      {
         "fileSystem":[
            "write",
            "directory",
            "retainEntries"
         ]
      }
   ],
   "optional_permissions":[
      "background"
   ],
   "sockets":{
      "udp":{
         "send":[
            ""
         ],
         "bind":[
            ""
         ],
         "multicastMembership":""
      },
      "tcp":{
         "connect":[
            ""
         ]
      },
      "tcpServer":{
         "listen":[
            "*:*"
         ]
      }
   }
}